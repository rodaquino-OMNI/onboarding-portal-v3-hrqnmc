# API Configuration
VITE_API_BASE_URL=https://api.austa.health
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000
VITE_AUTH_API_URL=https://auth.austa.health
VITE_MAX_RETRIES=3
VITE_ENABLE_MOCK_API=false

# Internationalization
VITE_DEFAULT_LANGUAGE=pt-BR
VITE_AVAILABLE_LANGUAGES=["pt-BR","en"]
VITE_TRANSLATION_PATH=/assets/i18n/

# Authentication & Security
VITE_ENABLE_MFA=true
VITE_TOKEN_EXPIRY=28800000
VITE_SESSION_DURATION=1800000
VITE_ENCRYPTION_KEY=your-encryption-key-here
VITE_CORS_ALLOWED_ORIGINS=["https://austa.health","https://admin.austa.health"]
VITE_CSP_DIRECTIVES={"default-src": "'self'","script-src": "'self' 'unsafe-inline'","style-src": "'self' 'unsafe-inline'","img-src": "'self' data: https:","connect-src": "'self' https://api.austa.health"}

# Rate Limiting
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=3600000

# LGPD Compliance
VITE_LGPD_CONSENT_VERSION=1.0.0
VITE_PRIVACY_POLICY_VERSION=1.0.0
VITE_AUDIT_LOG_ENABLED=true

# Performance & Monitoring
VITE_METRICS_ENABLED=true
VITE_ERROR_REPORTING_URL=https://errors.austa.health
VITE_CACHE_DURATION=7200000
VITE_HEALTH_CHECK_INTERVAL=30000

# Feature Flags
VITE_FEATURE_FLAGS={"enableHealthQuestionnaire":true,"enableDocumentUpload":true,"enableMFA":true}

# Maintenance
VITE_MAINTENANCE_MODE=false
VITE_MAINTENANCE_MESSAGE="Sistema em manutenção programada. Por favor, tente novamente em alguns minutos."

# Document Upload
VITE_MAX_UPLOAD_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=["application/pdf","image/jpeg","image/png"]
VITE_UPLOAD_CHUNK_SIZE=1048576

# Development Settings (do not use in production)
VITE_ENABLE_LOGGING=false
VITE_DEBUG_MODE=false