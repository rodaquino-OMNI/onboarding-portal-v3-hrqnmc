{
  "name": "austa-health-onboarding-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "workspaces": [
    "auth-service",
    "api-gateway",
    "document-service",
    "enrollment-service",
    "health-service",
    "policy-service"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "build:clean": "rimraf dist && npm run build",
    "test": "jest --config jest.config.ts --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "clean": "rimraf dist coverage",
    "prepare": "husky install",
    "security-audit": "npm audit --audit-level=high",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@kong/kong-config-ts": "^1.0.0",
    "@kong/kong-nodejs-sdk": "^1.0.0",
    "@opentelemetry/api": "^1.4.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "express-validator": "^7.0.1",
    "geoip-lite": "^1.4.7",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "http-status": "^1.7.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openapi-types": "^12.1.3",
    "opossum": "^7.1.0",
    "otplib": "^12.0.1",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "rate-limit-redis": "^3.0.0",
    "rate-limiter-flexible": "^3.0.0",
    "speakeasy": "^2.0.0",
    "swagger-ui": "^5.0.0",
    "twilio": "^4.18.0",
    "@twilio/client": "^4.19.0",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "29.5.0",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "cross-env": "7.0.3",
    "eslint": "8.40.0",
    "husky": "8.0.3",
    "ioredis-mock": "^8.2.2",
    "jest": "29.5.0",
    "nock": "^13.3.1",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "snyk": "latest",
    "supertest": "^6.3.3",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "^5.0.0"
  }
}